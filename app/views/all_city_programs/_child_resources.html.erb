<p>Some additional resources from Purple Binder:</p>

<% if @child_resources_zip.count >= 2 %>
  <ul>
    <li class="laf-headline"><%= @child_resources_zip.first.name %> </li>
    <li> <%= @child_resources_zip.first.street %>  <%= @child_resources_zip.first.city %> <%= @child_resources_zip.first.state %>, <%= @child_resources_zip.first.zip %> </li>
    <li><%= @child_resources_zip.first.phone %></li>
  </ul>

  <ul>
    <li class="laf-headline"><%= @child_resources_zip.second.name %> </li>
    <li> <%= @child_resources_zip.second.street %>  <%= @child_resources_zip.second.city %> <%= @child_resources_zip.second.state %>, <%= @child_resources_zip.second.zip %> </li>
    <li><%= @child_resources_zip.second.phone %></li>
  </ul>
<% end %>

<% if @child_resources_first.present? %>
  <ul>
    <li class="laf-headline"><%= @child_resources_first.name %> </li>
    <li> <%= @child_resources_first.street %>  <%= @child_resources_first.city %> <%= @child_resources_first.state %>, <%= @child_resources_first.zip %> </li>
    <li><%= @child_resources_first.phone %></li>
  </ul>

  <ul>
    <li class="laf-headline"><%= @child_resources_second.name %> </li>
    <li> <%= @child_resources_second.street %>  <%= @child_resources_second.city %> <%= @child_resources_second.state %>, <%= @child_resources_second.zip %> </li>
    <li><%= @child_resources_second.phone %></li>
  </ul>

<% end %>
