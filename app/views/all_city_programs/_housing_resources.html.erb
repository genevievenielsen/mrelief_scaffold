
<p>Some additional resources from Purple Binder:</p>

<% if @housing_resources_zip.count >= 2 %>
  <ul>
    <li class="laf-headline"><%= @housing_resources_zip.first.name %> </li>
    <li> <%= @housing_resources_zip.first.street %>  <%= @housing_resources_zip.first.city %> <%= @housing_resources_zip.first.state %>, <%= @housing_resources_zip.first.zip %> </li>
    <li><%= @housing_resources_zip.first.phone %></li>
  </ul>

  <ul>
    <li class="laf-headline"><%= @housing_resources_zip.second.name %> </li>
    <li> <%= @housing_resources_zip.second.street %>  <%= @housing_resources_zip.second.city %> <%= @housing_resources_zip.second.state %>, <%= @housing_resources_zip.second.zip %> </li>
    <li><%= @housing_resources_zip.second.phone %></li>
  </ul>
<% end %>

<% if @housing_resources_first.present? %>
  <ul>
    <li class="laf-headline"><%= @housing_resources_first.name %> </li>
    <li> <%= @housing_resources_first.street %>  <%= @housing_resources_first.city %> <%= @housing_resources_first.state %>, <%= @housing_resources_first.zip %> </li>
    <li><%= @housing_resources_first.phone %></li>
  </ul>

  <ul>
    <li class="laf-headline"><%= @housing_resources_second.name %> </li>
    <li> <%= @housing_resources_second.street %>  <%= @housing_resources_second.city %> <%= @housing_resources_second.state %>, <%= @housing_resources_second.zip %> </li>
    <li><%= @housing_resources_second.phone %></li>
  </ul>

<% end %>
