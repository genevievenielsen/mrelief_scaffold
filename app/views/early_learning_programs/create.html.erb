<div class="container marquee">
<%= render "layouts/print_button" %>
  
<% if @eligible == false %>
  <div class="row">
    <div class="col-md-7">
      <h1>Results: Something else in mind!</h1>
      <p>You likely do not qualify for Early Learning Programs</p>
    </div>
  </div>

<% else %>
  <div class="row" >
    <div class="col-md-7">
        <h1>Results: You may be in luck!</h1>
        <p> Below are Early Learning Programs that you are eligible for near you.</p>
    </div>
  </div>

  <% if @preferred_early_learning_programs.present? %>
  <div class="row">
    <div class="col-md-12">
    <h1>Preferred Early Learning Programs</h1>
    <br>
    <% @preferred_early_learning_programs.each do |program| %>
      <ul>
        <li class="laf-headline"> <%= program.funder %></li>
        <li>Agency: <%=  program.agency %></li>
        <li>Site: <%= program.site %></li>
        <li>Ages Served: <%= program.ages_served %></li>
        <li>Income Type: <%= program.income_type %></li>
        <% if program.income_type == "Greater than Type 2" %>
          <li>May have a required fee based on the income of the parents or legal guardians. Use the calculator[link to CPS] estimate your families co-pay.</li>
        <% end %>
        <li>Duration: <%= program.duration %></li>
        <li>Additional Criteria: <%= program.additional_criteria %></li>
        <% if program.funder.include?("Childcare Collaboration") && @ccap_eligible %>
          <li>This is a CCAP Compatible Program, and our results show that you also may qualify for that CCAP.</li>
        <% end %>
      </ul>
      <br>
    <% end %>
     
    </div>
  </div>
  <% end %>

  <div class="row">
    <div class="col-md-12">
    <h1>All Eligible Early Learning Programs</h1>
    <br>
    <% @eligible_early_learning_programs.each do |program| %>
      <ul>
        <li class="laf-headline"> <%= program.funder %></li>
        <li>Agency: <%=  program.agency %></li>
        <li>Site: <%= program.site %></li>
        <li>Ages Served: <%= program.ages_served %></li>
        <li>Income Type: <%= program.income_type %></li>
        <% if program.income_type == "Greater than Type 2" %>
          <li>May have a required fee based on the income of the parents or legal guardians. Use the calculator[link to CPS] estimate your families co-pay.</li>
        <% end %>
        <li>Duration: <%= program.duration %></li>
        <li>Additional Criteria: <%= program.additional_criteria %></li>
      </ul>
      <br>
    <% end %>
     
    </div>
  </div>
<% end %>

<%= link_to  "More Results: WIC & CCAP", more_results_url(@user) %>




</div>