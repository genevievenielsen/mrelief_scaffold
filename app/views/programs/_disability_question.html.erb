   <div class="row">
     <div class="col-md-9 col-sm-10 col-xs-12">
       <label for="disabled" id="disabled">Does your household include a member who is receiving disability benefits?</label>
       <div class="radio-button">
       <% [ 'Yes, from Social Security', 'Yes, from the Railroad Retirement Board ', 'Yes, from Veterans Affairs', 'No' ].each do |disabled| %>
       <label for="disabled_<%= disabled %>" class="checkbox-question">
         <%= radio_button_tag 'disabled', disabled, @current_user.try(:disability_benefits) || @d.try(:disabled_status) == disabled, id: "disabled_#{disabled}" %>
         <%= disabled.humanize %> </label><br>
       <% end %>
       </div>
     </div>
   </div>
